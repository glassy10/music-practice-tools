{
  "name": "music-practice-tools",
  "version": "0.1.0",
  "description": "Tools to help with practicing and improving musicality.",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf _site build",
    "spdev": "snowpack dev",
    "spbuild": "snowpack build",
    "zbuild": "eleventy",
    "build": "npm run clean && npm run spbuild",
    "watch": "eleventy --watch",
    "dev": "npm run clean && npm run spdev",
    "devz": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "devall": "nodemon --watch .eleventy.js --watch src/_11ty --exec \"eleventy --serve\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/music-practice-tools/music-practice-tools.git"
  },
  "author": {
    "name": "Steve Lee",
    "email": "steve@opendirective.com",
    "url": "https://opendirective..com/"
  },
  "license": "proprietary",
  "bugs": {
    "url": "https://opendirective.com"
  },
  "homepage": "https://musicpracticetools.net",
  "devDependencies": {
    "@11ty/eleventy": "0.11.0",
    "@11ty/eleventy-navigation": "0.1.6",
    "@tonaljs/tonal": "4.0.2",
    "abcjs": "6.0.0-beta.12",
    "alpinejs": "^2.4.1",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "luxon": "1.24.1",
    "markdown-it": "11.0.0",
    "markdown-it-anchor": "5.3.0",
    "markdown-it-block-embed": "0.0.3",
    "markdown-it-task-lists": "2.1.1",
    "nodemon": "2.0.4",
    "parcel": "^1.12.4",
    "prettier": "2.0.5",
    "shx": "^0.3.2",
    "snowpack": "2.6.3",
    "tone": "13.8.25"
  },
  "dependencies": {}
}
